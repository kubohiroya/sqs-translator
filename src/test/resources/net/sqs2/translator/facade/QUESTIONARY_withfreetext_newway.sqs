      <h>My First Test Questionnaire, about nothing at all 2.</h>
       <sqs:matrix-forms sqs:form-width-ratio="0.8">
    <xforms:hint>What fruit do you like and why do you like it?</xforms:hint>
    <sqs:row-array>
      <xforms:group>
        <xforms:hint>apple</xforms:hint>
      </xforms:group>
      <xforms:group>
        <xforms:hint>orange</xforms:hint>
      </xforms:group>
      <xforms:group>
        <xforms:hint>strawberry</xforms:hint>
      </xforms:group>
    </sqs:row-array>
   
    <sqs:column-array>
      <sqs:select>
        <xforms:item>
          <xforms:label>like</xforms:label>
          <xforms:value>1</xforms:value>
        </xforms:item>
        <xforms:item>
          <xforms:label>likez</xforms:label>
          <xforms:value>2</xforms:value>
        </xforms:item>
        <xforms:item>
          <xforms:label>eike</xforms:label>
          <xforms:value>3</xforms:value>
        </xforms:item>
        
      </sqs:select>
      <sqs:textarea sqs:width="200" sqs:height="20">
        <xforms:hint>reason</xforms:hint>
      </sqs:textarea>
    </sqs:column-array>
  </sqs:matrix-forms>

